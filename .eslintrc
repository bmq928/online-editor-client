{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-console": "off",
    "indent": ["off"],
    "linebreak-style": ["error", "unix"],
  },
  "globals": {
    "AppError": true,
    "logger": true
  }
}